package views

import ("github.com/lauritsbonde/LogLite/src/webApp/components")


templ Index(noConfig bool) {
  <!DOCTYPE html>
  <html lang="en">
      @components.Header()

      <body class="min-h-[100dvh]">
        @components.TopMenu()
        <section class="py-2 px-4">
          if noConfig {
            @components.Setup()
          } else {
            <p>Config</p>
          }
        </section>

      </body>

      @components.Footer()
  </html>
}
